{
    "type": "symfony-open-api-boilerplate",
    "license": "MIT",
    "authors": [
        {
            "name": "Sami Akkawi",
            "email": "sami@akkawi.ch",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ramsey/uuid": "^4.0",
        "symfony/console": "5.0.*",
        "symfony/dotenv": "5.0.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.0.*",
        "symfony/yaml": "5.0.*"
    },
    "require-dev": {
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts",
            "composer dump-autoload",
            "php bin/console cache:pool:clear cache.global_clearer -q"
        ],
        "post-update-cmd": [
            "@auto-scripts",
            "composer dump-autoload",
            "php bin/console cache:pool:clear cache.global_clearer -q"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.1.*"
        }
    }
}
